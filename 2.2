#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a, b, c, d, e, f;
    scanf("%d %d %d %d %d %d", &a, &b, &c, &d, &e, &f);
    float x, y;
    int del = b*d - a*e;

    if (del == 0) {
        if (c*d != f*a)
            printf("He phuong trinh vo nghiem");
        if (c*d == f*a)
            printf("He phuong trinh vo so nghiem");
    }
    else {
        y = (float)(c*d-a*f)/del;
        x = (float)(c-b*y)/a;
        printf("He phuong trinh co nghiem: %f %f", x, y);
    }

    return 0;
}
