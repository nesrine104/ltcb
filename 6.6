#include<stdio.h>
#define max 100

int main() {
    int a[max];
    int n;
    printf("Nhap so phan tu: ");
    scanf("%d", &n);
    for (int i=0; i<n; i++) {
        printf("a[%d]=", i);
        scanf("%d", &a[i]);
    }
    for (int i=0; i<n; i++) {
        if (a[i]<0) a[i]=0;
    }
    printf("Mang tm la: ");
    for(int i=0; i<n; i++) {
        printf("%d ", a[i]);
    }
    return 0;
}
