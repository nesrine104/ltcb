#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a[100];
    int n, x;
    int dn=0, dl=0;
    printf("Nhap so luong: ");
    scanf("%d", &n);
    printf("Nhap cac phan tu: ");
    for(int i=0; i<n; i++) {
        scanf("%d", &a[i]);
    }
    printf("Nhap phan tu x: ");
    scanf("%d", &x);
    for(int i=0; i<n; i++) {
        if(a[i]>x) dl++;
        if(a[i]<x) dn++;
    }
    printf("co %d phan tu lon hon %d la: ", dl, x);
    for(int i=0; i<n; i++) {
        if(a[i]>x) printf("%d ", a[i]);
    }
    printf("\nCo %d phan tu nho hon %d la: ", dn, x);
    for(int i=0; i<n; i++) {
        if(a[i]<x) printf("%d ", a[i]);
    }
    return 0;
}
