#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n, a[100];
    int cnt=0;
    printf("Nhap so luong: ");
    scanf("%d", &n);
    for(int i=0; i<n; i++) {
        scanf("%d", &a[i]);
    }
    int x;
    int max=0;
    for(int i=0; i<n; i++) {
        for(int j=0; j<n; j++) {
            if(a[j]==a[i]) cnt++;
        }
        
        if(cnt>max) {
            max=cnt;
            x=a[i];
        }
        cnt=0;
    }
    printf("Phan tu %d xuat hien nhieu nhat %d lan", x, max); 
    return 0;
}
