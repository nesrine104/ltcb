#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n, a[100];
    int cnt=0;
    printf("Nhap so luong: ");
    scanf("%d", &n);
    for(int i=0; i<n; i++) {
        scanf("%d", &a[i]);
    }
    int max=a[0];
    for(int i=0; i<n; i++) {
        if(a[i]>max) max=a[i];
    }
    for(int i=0; i<n; i++) {
        if(a[i]==max) cnt++;
    }
    printf("Phan tu lon nhat la %d xuat hien %d lan", max, cnt);
    return 0;
}
