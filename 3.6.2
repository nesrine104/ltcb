#include<stdio.h>
#include<math.h>

int main()
{
    int n;
    int s=0;
    printf("nhap so nguyen: ");
    scanf("%d", &n);
    for (int i=1; i<=n; i++) {
        if (n%i==0)
            s+=i;
    }
    if (s==2*n)
        printf("%d la so hoan hao", n);
    else printf("%d khong la so hoan hao", n);
    
    return 0;
}
