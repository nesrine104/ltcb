#include<stdio.h>
#define max 100

int main() {
    int A[100][100], B[100][100], C[100][100];
    int a, b, c, d, s=0;
    do {
        printf("Nhap so hang cua mt 1: ");
        scanf("%d", &a);
        printf("Nhap so cot cua mt 1: ");
        scanf("%d", &b);
        printf("Nhap so hang cua mt 2: ");
        scanf("%d", &d);
        printf("Nhap so cot cua mt 2: ");
        scanf("%d", &c);
        if (b!=d) printf("Khong the nhan, moi nhap lai:\n");
    }
    while (b!=d);
    printf("Nhap mang A:\n");
    for (int i=0; i<a; i++) {
        for (int j=0; j<b; j++) {
            printf("A[%d][%d]=", i, j);
            scanf("%d", &A[i][j]);
        }
    }
    printf("Nhap mang B:\n");
    for (int i=0; i<b; i++) {
        for (int j=0; j<c; j++) {
            printf("B[%d][%d]=", i, j);
            scanf("%d", &B[i][j]);
        }
    }
    for (int i=0; i<a; i++){
        for (int j=0; j<c; j++) {
            for (int k=0; k<b; k++) {
                s+=A[i][k]*B[k][j];
            }
            C[i][j]=s;
                s=0;
        }
    }
    printf("Mang A:\n");
    for(int i=0; i<a; i++) {
        for (int j=0; j<b; j++) {
            printf("%3d", A[i][j]);
        }
        printf("\n");
    }
    printf("Mang B:\n");
    for(int i=0; i<b; i++) {
        for (int j=0; j<c; j++) {
            printf("%3d", B[i][j]);
        }
        printf("\n");
    }
    printf("Mang tich:\n");
    for(int i=0; i<a; i++) {
        for (int j=0; j<c; j++) {
            printf("%3d", C[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
