#include<stdio.h>

int main()
{
    printf("Nhap so nguyen: ");
    int n;
    int gt=1;
    float s = 0;
    
    scanf("%d", &n);
    for (int i = 1; i<=n; i++) {
        for (int j=1; j<=i; j++) {
            gt *= j;
            
        }
        s += 1.0/gt;
        gt = 1;
    }
    printf("Ket qua S = %f", s);
    return 0;
    
}
