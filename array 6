#include<stdio.h>

int main()
{
    int n, a[100];
    printf("Nhap so phan tu: "); 
    scanf("%d", &n);
    for(int i=0; i<n; i++) {
        printf("Nhap phan tu %d: ", i+1);
        scanf("%d", &a[i]);
    }
    for(int i=0; i<n; i++) {
        if(a[i]<0) a[i]=0;
    }
    printf("Mang sau khi thay doi: ");
    for(int i=0; i<n; i++) {
        printf("%d ", a[i]);
    }
    return 0;
}
