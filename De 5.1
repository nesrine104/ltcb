#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    char a[100][100];
    int m, n;
    printf("Nhap kich thuoc hinh chu nhat: ");
    scanf("%d%d", &m, &n);
    for(int i=0; i<m; i++) {
        for(int j=0; j<n; j++) {
            if(i==0||i==m-1||j==0||j==n-1) {
                a[i][j]='*';
            }
            else {
                a[i][j]=' ';
            }
        }
        
    }
    for(int i=0; i<m; i++) {
        for(int j=0; j<n; j++) {
            printf("%c", a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
