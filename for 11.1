#include <stdio.h>

int main()
{
    int n;
    int i=0;
    int a[100];
    printf("Nhap so nguyen: ");
    scanf("%d", &n);
    printf("Co so 2: ");
    if(n<2) printf("%d", n);
    else {
        while (n>0) {
            a[i]=n%2;
            n/=2;
            i++;
        }
        for(int j=i-1; j>=0; j--) {
            printf("%d", a[j]);
        }
    }
    return 0;
}
