#include<stdio.h>
#include<math.h>

int main() 
{
    int m, y;
    int a;
    scanf("%d %d", &m, &y);
    if (m<1||m>12)
        printf("Thang %d nam %d co %d ngay", m, y, a);
    else {
    
        if (m==1||m==3||m==5||m==7||m==8||m==10||m==12)
            a = 31;
        else if (m==4||m==6||m==9||m==11)
            a = 30;
