#include<stdio.h>
#include<stdlib.h>

int main()
{
    char a;
    printf("Nhap diem sinh vien: ");
    scanf("%c", &a);
    switch(a)
    {
        case 'A': printf("Sinh vien xuat sac"); break;
        case 'B': printf("Sinh vien gioi"); break;
        case 'C': printf("Sinh vien kha"); break;
        case 'D': printf("Sinh vien trung binh"); break;
        case 'F': printf("Sinh vien yeu"); break;
        default: printf("Khong the phan loai"); break;
    }
    return 0;
}
