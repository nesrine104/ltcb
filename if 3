#include<stdio.h>
#include<stdlib.h>
#include<math.h>

int main()
{
    int dtb;
    printf("Nhap diem trung binh: ");
    scanf("%d", &dtb);
    if(dtb<0||dtb>10) printf("khong hop le");
    else {
        if(dtb<5) printf("Yeu");
        else if(dtb<7) printf("Trung binh");
        else if(dtb<8) printf("Kha");
        else if(dtb<9) printf("Gioi");
        else printf("Xuat sac");
    }
    return 0;
}
