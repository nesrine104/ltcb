#include<stdio.h>

int main()
{
    int a[10];
    for(int i=0; i<10; i++) {
        printf("Nhap phan tu thu %d: ", i+1);
        scanf("%d", &a[i]);
    }
    printf("Mang da nhap la: ");
    for(int i=0; i<10; i++) {
        printf("%d ", a[i]);
    }
    printf("\nMang duoc in dao nguoc: ");
    for(int i=9; i>=0; i--) {
        printf("%d ", a[i]);
    }
    return 0;
}
