#include<stdio.h>

int main()
{
    int n, a[100];
    int x;
    int dem=0;
    printf("Nhap so phan tu: "); 
    scanf("%d", &n);
    for(int i=0; i<n; i++) {
        printf("Nhap phan tu %d cua mang: ", i+1);
        scanf("%d", &a[i]);
    }
    printf("Nhap phan tu can dem: ");
    scanf("%d", &x);
    for(int i=0; i<n; i++) {
        if(a[i]==x)
            dem++;
    }
    printf("Phan tu %d xuat hien %d lan", x, dem);
    return 0;
}
