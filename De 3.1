#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    int n;
    printf("Nhap so nguyen n: ");
    scanf("%d", &n);
    int check=0;
    if(n==0||n==1) printf("Khong co");
    else {
        printf("So nguyen to nho hon n la: ");
        for(int i=2; i<n; i++) {
            for(int j=2; j<=sqrt(i); j++) {
                if(i%j==0) 
                    check++;
            }
            if(check==0) printf("%d ", i);
            check=0;
        }
    }
    return 0;
}
