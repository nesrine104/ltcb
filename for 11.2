#include<stdio.h>

int main()
{
    int n;
    int a[100];
    printf("Nhap so nguyen: ");
    scanf("%d", &n);
    int i=0;
    printf("Co so 8: ");
    if(n<8) printf("%d", n);
    else {
        while (n!=0) {
            a[i]=n%8;
            n/=8;
            i++;
        }
        for(int j=i-1; j>=0; j--){
            printf("%d", a[j]);
        }
    }
    return 0;
}
